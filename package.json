{
  "name": "grid-protocol",
  "version": "1.2.56",
  "description": "Grid protocol descriptors automatically generated from the grid-fw repository!",
  "scripts": {
    "test": "vitest run",
    "clean": "rm -rf dist",
    "build": "webpack --mode production",
    "prepack": "npm run build",
    "prepublishOnly": "npm run build && npm run remove-deps",
    "remove-deps": "node remove-dependencies.js",
    "postinstall": "echo 'Patching dependencies...' && patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intechstudio/grid-protocol.git"
  },
  "keywords": [
    "intech",
    "studio",
    "grid"
  ],
  "author": "intech studio",
  "bugs": {
    "url": "https://github.com/intechstudio/grid-protocol/issues"
  },
  "homepage": "https://github.com/intechstudio/grid-protocol#readme",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "lodash": "^4.17.21",
    "lua-fmt": "^2.6.0",
    "luamin": "^1.0.4"
  },
  "devDependencies": {
    "patch-package": "^8.0.0",
    "@types/lodash": "^4.17.4",
    "babel-loader": "^8.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^4.10.0"
  }
}
